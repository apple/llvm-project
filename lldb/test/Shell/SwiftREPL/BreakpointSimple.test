// Test that we can set breakpoints in the REPL.
// REQUIRES: swift
// RUN: %lldb --repl -O "log enable gdb-remote packets" -O "log enable lldb breakpoints" < %s | FileCheck %s

func foo() -> Int {
  return 0
}

:break set -n foo
// CHECK: Breakpoint 1: {{.*}}foo() {{.*}} repl.swift:6:3, address = 0x

// CHECK: Execution stopped at breakpoint
foo()
